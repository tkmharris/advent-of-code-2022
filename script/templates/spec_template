require_relative '../answers/answers'
require_relative "../../solutions/day_<<day>>"

describe Day<<day>> do
  describe '#part_1' do
    subject do
      Day<<day>>.new.part_1
    end

    it 'gives the correct answer on the problem input data' do
      expect(subject).to eq(ANSWERS[:day_<<day>>][:part_1])
    end
  end

  describe '#part_2' do
    subject do
      Day<<day>>.new.part_2
    end

    it 'gives the correct answer on the problem input data' do
      expect(subject).to eq(ANSWERS[:day_<<day>>][:part_2])
    end
  end
end